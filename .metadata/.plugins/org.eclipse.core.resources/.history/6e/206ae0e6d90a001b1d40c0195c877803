#include "ee.h"
#include "Arduino.h"
#include "MyArduino.h"
#include "stdio.h"

void StartupHook() {
	char buff[BUFFSIZ];

	DDRD |= (MASK(3) | MASK(7));
	PORTD &= ~(MASK(3) | MASK(7));


	header_serial_print(__FILE__, __FUNCTION__);
	Serial.print("PORTD = 0b");
	Serial.println(PORTD, BIN);

	//sprintf(buff, "hook.cpp: DDRD=[0x%x]\tPORTD=[0x%x]", DDRD, PORTD);
	//Serial.println(buff);
}
